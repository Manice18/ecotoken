{
    "semi": true,
        "tabWidth": 2,
            "useTabs": false,
                "endOfLine": "lf",
                    "singleQuote": true,
                        "jsxSingleQuote": true,
                            "trailingComma": "es5",
                                "bracketSpacing": true,
                                    "bracketSameLine": false,
                                        "arrowParens": "always",
                                            "singleAttributePerLine": false,
                                                "htmlWhitespaceSensitivity": "strict",
                                                "plugins": ["prettier-plugin-tailwindcss"]
}
